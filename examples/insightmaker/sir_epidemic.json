{
  "name": "SIR Epidemic (InsightMaker Format)",
  "settings": {
    "start": 0,
    "stop": 100,
    "dt": 0.25,
    "time_units": "days"
  },
  "primitives": [
    {
      "id": "s1",
      "type": "Stock",
      "name": "Susceptible",
      "value": "990",
      "outflows": ["f1"],
      "units": "people"
    },
    {
      "id": "s2",
      "type": "Stock",
      "name": "Infected",
      "value": "10",
      "inflows": ["f1"],
      "outflows": ["f2"],
      "units": "people"
    },
    {
      "id": "s3",
      "type": "Stock",
      "name": "Recovered",
      "value": "0",
      "inflows": ["f2"],
      "units": "people"
    },
    {
      "id": "f1",
      "type": "Flow",
      "name": "infection_rate",
      "equation": "contact_rate * infectivity * Susceptible * Infected / total_population",
      "units": "people/day"
    },
    {
      "id": "f2",
      "type": "Flow",
      "name": "recovery_rate",
      "equation": "Infected / recovery_time",
      "units": "people/day"
    },
    {
      "id": "a1",
      "type": "Converter",
      "name": "total_population",
      "equation": "Susceptible + Infected + Recovered",
      "units": "people"
    },
    {
      "id": "p1",
      "type": "Parameter",
      "name": "contact_rate",
      "value": "5",
      "units": "contacts/person/day"
    },
    {
      "id": "p2",
      "type": "Parameter",
      "name": "infectivity",
      "value": "0.25",
      "units": "dimensionless"
    },
    {
      "id": "p3",
      "type": "Parameter",
      "name": "recovery_time",
      "value": "10",
      "units": "days"
    }
  ]
}
